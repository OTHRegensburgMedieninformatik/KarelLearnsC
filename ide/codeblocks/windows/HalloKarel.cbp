				<Linker>
					<Add library="mingw32" />
					<Add library="lib/libfreetype-6.dll" />
					<Add library="lib/libjpeg-8.dll" />
					<Add library="lib/libkarel.a" />
					<Add library="lib/libpng15-15.dll" />
					<Add library="lib/libSDLmain.a" />
					<Add library="lib/libtiff-5.dll" />
					<Add library="lib/libwebp-2.dll" />
					<Add library="lib/SDL.dll" />
					<Add library="lib/SDL_image.dll" />
					<Add library="lib/SDL_ttf.dll" />
					<Add library="lib/zlib1.dll" />
				</Linker>
				<ExtraCommands>
					<Add after="cmd /c copy $(PROJECT_DIR)lib\*.dll $(TARGET_OUTPUT_DIR)" />
